{
  "expo": {
    "name": "my-expo-app",
    "slug": "my-expo-app",
    "version": "1.0.0",

    

    "experiments": {
      "tsconfigPaths": true
    },

    "plugins": [],

    "orientation": "portrait",
    "icon": "./assets/mora.png",

    "userInterfaceStyle": "light",

    
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.example.myexpoapp",
      "config": {
        "googleSignIn": {
          "reservedClientId": "REPLACE_WITH_IOS_REVERSED_CLIENT_ID"
        }
      }
    },
    "android": {
      "package": "com.example.myexpoapp",
      "intentFilters": [
        {
          "action": "VIEW",
          "data": {
            "scheme": "myexpoapp"
          },
          "category": ["BROWSABLE", "DEFAULT"]
        }
      ]
      
    },
    "scheme": "myexpoapp",
    "extra": {
      "expoClientId": "REPLACE_WITH_WEB_CLIENT_ID.apps.googleusercontent.com",
      "iosClientId": "REPLACE_WITH_IOS_CLIENT_ID.apps.googleusercontent.com",
      "androidClientId": "REPLACE_WITH_ANDROID_CLIENT_ID.apps.googleusercontent.com"
    }
  }
}
